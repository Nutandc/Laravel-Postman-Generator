{
    "name": "nutandc/laravel-postman-generator",
    "type": "library",
    "description": "Laravel Postman & OpenAPI Generator â€” auto-create Postman collections and OpenAPI 3.0 docs from Laravel routes with smart grouping, examples, and environment files.",
    "license": "MIT",
    "homepage": "https://github.com/Nutandc/Laravel-Postman-Generator",
    "keywords": [
        "Laravel",
        "Postman",
        "OpenAPI",
        "Documentation",
        "API",
        "Generator"
    ],
    "autoload": {
        "psr-4": {
            "Nutandc\\PostmanGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nutan Dahal",
            "email": "info@dahalnutan.com.np"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.92",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpstan/phpstan": "^1.11",
        "phpstan/phpstan-phpunit": "^1.4"
    },
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse --configuration=phpstan.neon",
        "fix": "php-cs-fixer fix",
        "fix:dry": "php-cs-fixer fix --dry-run --diff"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Nutandc\\PostmanGenerator\\Providers\\PostmanGeneratorServiceProvider"
            ]
        }
    }
}
